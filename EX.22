#include <stdio.h>

int main() {
    int n;
    
    printf("Input the number of elements to be stored in the array: ");
    scanf("%d", &n);
    
    int arr[n];
    printf("Input %d elements in the array:\n", n);
    for(int i = 0; i < n; i++) {
        printf("element - %d : ", i);
        scanf("%d", &arr[i]);
    }
    
    int visited[n];
    for(int i = 0; i < n; i++) {
        visited[i] = 0;
    }
    
    printf("The frequency of all elements of an array:\n");
    for(int i = 0; i < n; i++) {
        if(visited[i] == 0) {
            int count = 1;
            for(int j = i + 1; j < n; j++) {
                if(arr[i] == arr[j]) {
                    count++;
                    visited[j] = 1;
                }
            }
            printf("%d occurs %d times\n", arr[i], count);
        }
    }
    
    return 0;
}
